load R0 10            ; Loads value 10 into register 0
load R1 1             ; Loads value 1 into register 1
sub R0 R1 R0          ; Subtracts R1 (1) to R0 (10) and sets the output to register R0
call PRINTLN R0       ; System Call for 'PRINTLN' (0x00) and prints R0
halt                  ; Stops the program (since we didn't provide any register or value it will exit with 0 as exit code)

